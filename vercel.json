{
  "version": 2,
  "builds": [
    {
      "src": "api/handler.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/api/search",
      "dest": "/api/handler"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|txt|xml))",
      "dest": "/static/$1"
    },
    {
      "src": "/",
      "dest": "/static/index.html"
    }
  ]
}